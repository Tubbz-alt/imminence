<%= semantic_form_for(@scheme, :url => admin_business_support_scheme_path(@scheme)) do |f| %>
  <%= f.inputs do %>
    <%= f.input :title %>
    <%= f.input :business_support_identifier %>
  <% end %>

  <%= f.buttons do %>
    <%= f.commit_button "Update Business Support", :button_html => {:class => 'btn btn-primary'} %>
  <% end %>
<% end %>
